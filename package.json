{
  "name": "url-shortener",
  "version": "1.0.0",
  "description": "Serverless url shortener",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "test": "npx mocha --recursive tests",
    "test:watch": "npx mocha -w --parallel --recursive tests",
    "test:watchGrep": "npx mocha -w --parallel --recursive tests --grep",
    "lint": "npx eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zacbe/url-shortener.git"
  },
  "author": "@zacbe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zacbe/url-shortener/issues"
  },
  "homepage": "https://github.com/zacbe/url-shortener#readme",
  "devDependencies": {
    "eslint": "^8.39.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^1.7.1",
    "prettier": "^2.8.8"
  },
  "dependencies": {
    "@middy/core": "^4.4.0",
    "@middy/http-error-handler": "^4.4.0",
    "@middy/http-header-normalizer": "^4.4.0",
    "@middy/http-json-body-parser": "^4.4.0",
    "@middy/validator": "^4.4.0",
    "mongodb": "^5.3.0"
  }
}
